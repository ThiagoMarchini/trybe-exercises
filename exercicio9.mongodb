// Retorne os filmes da categoria adventure e com ratings maior do que 103 , exibindo apenas os campos title , ratings e category

use("class");

db.movies.find(
  {$and: [
    {category: {$all: ["adventure"]}},
    {ratings: {$elemMatch: {$gte: 103}}},
  ]},
  {
    _id: 0,
    title: 1,
    ratings: 1,
    category: 1,
  }
)